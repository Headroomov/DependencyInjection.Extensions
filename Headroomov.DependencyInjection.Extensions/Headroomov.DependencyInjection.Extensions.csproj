<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net10.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<RootNamespace>Headroomov.DependencyInjection.Extensions</RootNamespace>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageId>Headroomov.DependencyInjection.Extensions</PackageId>
		<Version>10.0.1</Version>
		<Description>A strict dependency injection extension package that ensures consistent service registration by enforcing matching lifetimes between interfaces and implementations through the use of ServiceLifetimeAttribute, preventing mismatches and promoting reliable, predictable DI configuration.</Description>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<RepositoryUrl>https://github.com/Headroomov/DependencyInjection.Extensions</RepositoryUrl>
		<PackageReadmeFile>README.md</PackageReadmeFile>
	</PropertyGroup>
	
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="10.0.2" />
  </ItemGroup>

</Project>
